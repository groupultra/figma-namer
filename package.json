{
  "name": "figma-namer",
  "version": "0.2.0",
  "description": "AI-powered semantic layer naming for Figma - Plugin + Web Dashboard",
  "scripts": {
    "start": "concurrently -n server,web -c blue,green \"tsx server/src/index.ts\" \"vite web\"",
    "start:server": "tsx server/src/index.ts",
    "start:web": "vite web",
    "build:plugin": "webpack --mode=production",
    "dev:plugin": "webpack --mode=development --watch",
    "build:web": "vite build web",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/ --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.100.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.1.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "inline-chunk-html-plugin": "^1.1.1",
    "jsdom": "^28.1.0",
    "mini-css-extract-plugin": "^2.7.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.5.0",
    "tsx": "^4.19.0",
    "typescript": "^5.3.0",
    "vite": "^6.0.0",
    "vitest": "^1.0.0",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.0",
    "@google/genai": "^0.7.0",
    "@napi-rs/canvas": "^0.1.65",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "openai": "^4.20.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
